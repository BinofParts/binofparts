<% @requests.each do |request| %>
	s = '<td class="media"><div class="left"><div class="imgcenter"></div><%= image_tag("kop#{request.part.year}/#{request.part.picture}", class: "media-object") %></div><div class="media-body"><h4 class="media-heading"><%= request.part.name %> <small>Quantity <b><%=  request.qty %></b></small></h4><p><%= request.part.description %></p></div><div class="right"><p>Team <%= request.team.team_number %></p><p>Team <%= request.team.nickname %></p></div></td>';

	newrow = document.createElement( "tr" );
	newrow.innerHTML = s;

 	$("#live-feed").prepend(newrow);

<% end %>