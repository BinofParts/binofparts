document.getElementById("new-request-parts-list").innerHTML = "";

document.getElementById('request-list-back').style.display='none';

s = '<%= @part.name %><%= form_for([@event, @event.requests.build]) do |f| %><%= f.hidden_field :part_id, :value => "#{@part_id}" %><%= f.number_field :qty %><%= f.hidden_field :team_id, :value => "#{current_user.team_number_id}" %><div class="modal-footer"><button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button><%= f.submit "Submit", :class => "btn btn-primary disabled" %></div><% end %>';

div = document.createElement( "div" );
div.innerHTML = s;

$('#new-request-parts-list').append(div);