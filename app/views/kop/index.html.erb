<% content_for :title, "- Kit of Parts" %>
<% track_event("Loaded Kit of Parts") %>
<div class="jumbotron">
	<div class="container">
		<div class="row">
			<div class="col-sm-4 col-sm-offset-2">
				<%= link_to image_tag("FRCicon.gif", class: "FRCicon"), "http://www.usfirst.org/roboticsprograms/frc", { :target=>"_blank" } %>
			</div>
			<div class="col-sm-4 tba">
				<i>Team & Event Data Provided By</i>
				<div class="tba-logo"><%= link_to (image_tag("tba_lamp.png")+"The Blue Alliance").html_safe, "http://thebluealliance.com", { :target=>"_blank"} %></div>
			</div>
		</div>
	</div>
</div>
<div class="container kop-gamelist">
	<div class="row">
	  <div class="col-xs-12 col-sm-6">
