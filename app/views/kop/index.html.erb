	  	<%= link_to image_tag("2013-Game.jpg", class: "kop-game"), kop_year_path(:id => "2013"), { :class=>"kop-thumb thumbnail" } %>
	  	<%= link_to image_tag("2012-Game.jpg", class: "kop-game"), kop_year_path(:id => "2012"), { :class=>"kop-thumb thumbnail" } %>
	  </div>
	  <div class="col-xs-12 col-sm-6 ">
	  	<%= link_to image_tag("2011-Game.jpg", class: "kop-game"), kop_year_path(:id => "2011"), { :class=>"kop-thumb thumbnail" } %>
	  	<%= link_to image_tag("2010-Game.jpg", class: "kop-game"), kop_year_path(:id => "2010"), { :class=>"kop-thumb thumbnail" } %>
	  	<%= link_to image_tag("2009-Game.jpg", class: "kop-game"), kop_year_path(:id => "2009"), { :class=>"kop-thumb thumbnail" } %>
	  </div>
	</div>
</div>