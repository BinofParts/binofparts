<h1>Listing teams</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.first(50).each do |team| %>
      <tr>
        <td><%= team.team_number %></td>
        <td><%= team.nickname %></td>
        <td><%= team.location %></td>
        <td><%= link_to 'Show', team %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>